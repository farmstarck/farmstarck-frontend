{
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://farmstarck-backend.onrender.com/api/:path*" }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "farmstarck.com" }],
      "destination": "https://www.farmstarck.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}